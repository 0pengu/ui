{
  "name": "default",
  "dependencies": [
    "tailwindcss-animate",
    "class-variance-authority",
    "clsx",
    "tailwind-merge",
    "lucide-react",
    ""
  ],
  "registryDependencies": [],
  "tailwind": {
    "config": {
      "theme": {
        "extend": {
          "borderRadius": {
            "lg": "var(--radius)",
            "md": "calc(var(--radius) - 2px)",
            "sm": "calc(var(--radius) - 4px)"
          }
        }
      },
      "plugins": [
        "require(\"tailwindcss-animate\")"
      ]
    }
  },
  "files": [],
  "cssVariables": {
    "light": {
      "--radius": "0.5rem"
    },
    "dark": {
      "--radius": "0.5rem"
    }
  }
}