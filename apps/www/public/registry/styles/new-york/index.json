{
  "name": "new-york",
  "type": "registry:style",
  "dependencies": [
    "tailwindcss-animate",
    "class-variance-authority",
    "lucide-react",
    "@radix-ui/react-icons"
  ],
  "registryDependencies": [
    "utils",
    "hello-block",
    "command"
  ],
  "tailwind": {
    "config": {
      "theme": {
        "extend": {
          "borderRadius": {
            "lg": "var(--radius)",
            "md": "calc(var(--radius) - 2px)",
            "sm": "calc(var(--radius) - 4px)"
          }
        }
      },
      "plugins": [
        "require(\"tailwindcss-animate\")"
      ]
    }
  },
  "cssVars": {
    "light": {
      "--radius": "0.5rem"
    }
  },
  "files": []
}